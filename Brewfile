#Misky Homebrew-Caskfile
#Install brew global command-line apps
brew 'brew-cask'
brew 'mongodb'
brew 'tmux'
brew 'wgete

#Install Cask - For installing GUI applications
tap 'caskroom/cask'

#Cask installation dir
cask_args appdir: '/Applications'

# Install Cask's GUI-apps
cask 'caffeine'
cask 'flux'
cask 'spotify'
cask 'dash'
cask 'vlc'
cask 'google-chrome'
cask 'spectacle'
cask 'iterm2'
cask 'virtualbox'
